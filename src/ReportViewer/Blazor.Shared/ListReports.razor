@inherits ListReportsBase


<h1>Reports</h1>

@if (Blobs == null)
{
    <p><em>Loading...</em></p>
}
else
{
    <table class="table">
        <thead>
            <tr>
                <th>Date</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var blob in Blobs)
            {
                <tr>
                    <td>@blob</td>
                    <td>
                        <button class="btn btn-primary" @onclick="@(() => ViewReport(blob))">View</button>
                    </td>
                </tr>
            }
        </tbody>
    </table>
}